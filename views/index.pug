extends layout

block content
  nav
    h1= title
    a(href='/')= 'Home'
    a(href='/about')= 'About'
    a(href='/contact')= 'Contact'
  section(id='sm_about')
    p Placeholder for image
    br
    p Small about the blogger section 

  section(id='blogs')
    p Blog list Section
    each val in data
      section(class='card')
        h2= val.blog_title
        p= val.blog_text
    br
  

  